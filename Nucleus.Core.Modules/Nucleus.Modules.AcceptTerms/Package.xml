  </description>
  <compatibility minVersion="3.0.0.0" maxVersion="3.*" />
  <components>
    <component folderName="AcceptTerms" optional="false">
      <moduleDefinition id="45963833-c2b2-4673-a25b-85ac7b2b5b49">
        <friendlyName>Accept Terms</friendlyName>
        <helpUrl>https://www.nucleus-cms.com/documentation/modules/accept-terms/</helpUrl>
        <extension>AcceptTerms</extension>
        <viewController>AcceptTerms</viewController>
        <settingsController>AcceptTermsAdmin</settingsController>
        <viewAction>Index</viewAction>
        <editAction>Settings</editAction>
        <categories>Content</categories>
      </moduleDefinition>
      <file name="readme.txt" />
      <file name="license.txt" />
      <folder name="bin">
        <file name="Nucleus.Modules.AcceptTerms.dll" />
      </folder>
      <cleanup>
        <file name="viewer.css" />
        <file name="settings.css" />
        <folder name="Views">
          <file name="Viewer.cshtml" />
          <file name="Settings.cshtml" />
        </folder>
      </cleanup>
    </component>
  </components>
</package>