<?xml version="1.0" encoding="utf-8"?>
<package id="3772d87d-5e44-4b0c-a2d3-3bb3ac83a0d7" xmlns="urn:nucleus/schemas/package/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:nucleus/schemas/package/1.0 http://www.nucleus-cms.com/schemas/package.xsd">
